{
  "name": "shortest",
  "version": "1.0.0",
  "description": "Simple URL shortener with SQLite3 as a database management system.",
  "scripts": {
    "start": "npm run start -w @shortest/client & npm run start -w @shortest/server",
    "dev": "npm run dev -w @shortest/client & npm run dev -w @shortest/server",
    "test": "jest",
    "lint": "npx eslint . --ext .js,.ts,.tsx",
    "lint:fix": "npx eslint . --ext .js,.ts,.tsx --fix"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.31.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-react": "^7.24.0",
    "jest": "^27.0.6",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": "21.x"
  },
  "repository": {
    "url": "https://github.com/dayvidwhy/shortest"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "express",
    "react"
  ],
  "private": true,
  "workspaces": [
    "./packages/*",
    "./apps/*"
  ]
